{
    "name": "inviqa/disqus-bundle",
    "description": "Disqus Integration Bundle",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^2.6|^3.0",
        "symfony/twig-bridge": "^2.6|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Inviqa\\DisqusBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inviqa\\DisqusBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
